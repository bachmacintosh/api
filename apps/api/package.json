{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm check-types && wrangler deploy --dry-run --outdir dist",
    "check-types": "tsc --noEmit",
    "codegen": "graphql-codegen-esm --require dotenv/config --config ./src/gql/codegen.ts dotenv_config_path=.dev.vars",
    "deploy": "pnpm check-types && wrangler deploy",
    "dev": "wrangler dev",
    "dev:cron": "wrangler dev --test-scheduled",
    "generate": "drizzle-kit generate:sqlite",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "wrangler d1 migrations apply api"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@bachmacintosh/api-types": "workspace:*",
    "@cloudflare/workers-types": "4.20240512.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typed-document-node": "5.0.6",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "discord-api-types": "0.37.84",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.20.17",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "5.3.3",
    "undici": "6.6.2",
    "wrangler": "3.53.1"
  },
  "dependencies": {
    "@discordjs/rest": "2.2.0",
    "cloudflare-workers-and-google-oauth": "2.0.0",
    "drizzle-orm": "0.30.10",
    "graphql": "16.8.1",
    "itty-router": "4.2.2",
    "mdast-util-from-markdown": "2.0.0",
    "mdast-util-gfm": "3.0.0",
    "mdast-util-to-markdown": "2.1.0",
    "micromark-extension-gfm": "3.0.0",
    "url-template": "3.1.1"
  }
}
